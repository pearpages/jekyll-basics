---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Test Blog</title>
	<link href="http://pearpages.github.io/understanding-immutablejs/blog/atom.xml" rel="self"/>
	<link href="http://pearpages.github.io/understanding-immutablejs/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://pearpages.github.io/understanding-immutablejs/blog</id>
	<author>
		<name>PearPages</name>
		<email>hello@pearpages.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://pearpages.github.io/understanding-immutablejs{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://pearpages.github.io/understanding-immutablejs{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>